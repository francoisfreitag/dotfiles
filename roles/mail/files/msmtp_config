account mail@franek.fr
host mail.ouvaton.coop
port 465
from mail@franek.fr
user mail@franek.fr
passwordeval "secret-tool lookup name mail@franek.fr"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile ~/.config/msmtp/msmtp.log
tls_starttls off

account francois.freitag@inclusion.gouv.fr
host smtp.gmail.com
port 465
from francois.freitag@inclusion.gouv.fr
user francois.freitag@inclusion.gouv.fr
passwordeval "secret-tool lookup name francois.freitag@inclusion.gouv.fr"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile ~/.config/msmtp/msmtp.log
tls_starttls off
